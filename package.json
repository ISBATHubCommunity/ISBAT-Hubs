{
  "name": "isbat-hubs-graphql-api",
  "version": "0.0.0",
  "description": "Graphql api for ISBAT Hubs community",
  "main": "index.js",
  "author": "Charles Elloit",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon ./index.js --exec babel-node",
    "prepare": "husky install",
    "pkg-upgrade": "yarn upgrade-interactive --latest",
    "lint": "node ./node_modules/eslint/bin/eslint . --ignore-path .gitignore",
    "lint-fix": "node ./node_modules/eslint/bin/eslint . --fix --ext .js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --forceExit",
    "ci:test": "cross-env NODE_ENV=test jest --ci --runInBand --detectOpenHandles --forceExit",
    "eslint:github-action": "node ./node_modules/eslint/bin/eslint . --ignore-path .gitignore --max-warnings 0"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.1",
    "@graphql-tools/utils": "^8.10.0",
    "apollo-server": "^3.10.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.5.0",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.10.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.2",
    "subscriptions-transport-ws": "^0.11.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.2"
  }
}
